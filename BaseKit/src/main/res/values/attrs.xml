<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StateView">
        <attr name="sv_loadingView" format="reference" />
        <attr name="sv_errorView" format="reference" />
        <attr name="sv_errorNetView" format="reference" />
        <attr name="sv_emptyView" format="integer" />
        <attr name="sv_retry" format="boolean" />
        <attr name="sv_viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="loading" value="-1" />
            <enum name="error" value="-2" />
            <enum name="error_net" value="-3" />
            <enum name="empty" value="-4" />
        </attr>
    </declare-styleable>
</resources>